print("test 6: nested loops")

is_prime = function(n)
    if n < 2 then
        return false
    end
    i = 2
    while i * i <= n do
        if n % i == 0 then
            return false
        end
        i = i + 1
    end
    return true
end

print("is_prime(7) = " + tostring(is_prime(7)))
print("is_prime(9) = " + tostring(is_prime(9)))
print("is_prime(2) = " + tostring(is_prime(2)))
