// spawn_profile_test.du - minimal spawn to identify bottleneck
ctx = context()
if ctx == nil then
  // Just spawn 100 workers, measure total time
  start = now()
  for i = 1, 100 do
    spawn("spawn_worker.du", {worker_id = i})
  end
  elapsed = now() - start
  print("Spawned 100 workers in " + elapsed + "ms")
  print("Per-spawn overhead: " + (elapsed / 100) + "ms")
  sleep(5)
end
